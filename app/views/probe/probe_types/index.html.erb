<h1>Listing probe_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>ptype</th>
  </tr>

<% @probe_types.each do |probe_type| %>
  <tr>
    <td><%= probe_type.name %></td>
    <td><%= probe_type.ptype %></td>
    <td><%= link_to 'Show', probe_type %></td>
    <td><%= link_to 'Edit', edit_probe_probe_type_path(probe_type) %></td>
    <td><%= link_to 'Destroy', probe_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New probe_type', new_probe_probe_type_path %>